<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:InventoryManagementGUI.ViewModel" 
                    xmlns:view="clr-namespace:InventoryManagementGUI.View">
    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SupplierVM}">
        <view:Suppliers />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductVM}">
        <view:Products />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductManagerVM}">
        <view:ProductsManager />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:OrderVM}">
        <view:Orders />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TransactionVM}">
        <view:Transactions />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingVM}">
        <view:Settings />
    </DataTemplate>

</ResourceDictionary>